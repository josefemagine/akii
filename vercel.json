{
  "version": 2,
  "framework": "vite",
  "functions": {
    "api/bedrock/index.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/bedrock/instances.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/bedrock/provision-instance.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/bedrock/delete-instance.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/bedrock",
      "dest": "/api/bedrock/index.js"
    },
    {
      "src": "/api/bedrock/instances",
      "dest": "/api/bedrock/instances.js"
    },
    {
      "src": "/api/bedrock/provision-instance",
      "dest": "/api/bedrock/provision-instance.js"
    },
    {
      "src": "/api/bedrock/delete-instance",
      "dest": "/api/bedrock/delete-instance.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}