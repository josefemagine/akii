  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "postbuild": "node scripts/generate-sitemap.js",
    "build:nocheck": "vite build",
    "build:skip-errors": "tsc --noEmit --skipLibCheck || vite build",
    "build-with-type-check": "tsc ; vite build",
    "build-no-errors": "tsc ; vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test-api": "node test-api.js",
    "api": "node api-server.js",
    "simple-api": "node simple-express-api.js",
    "types:supabase": "npx supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/types/supabase.ts",
    "version:patch": "node scripts/version.js patch",
    "version:minor": "node scripts/version.js minor",
    "version:major": "node scripts/version.js major",
    "version:list": "git tag --list 'v*' --sort=-v:refname",
    "setup-admin": "node src/scripts/setup-admin.js",
    "update-admin": "node src/scripts/update-admin.js",
    "run-migrations": "supabase db push",
    "dev:with-api": "concurrently \"npm run dev\" \"npm run simple-api\"",
    "deploy:stripe-api": "supabase functions deploy stripe-api"
